<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="LibraryManager.AdminList"
        Title="Выбор LibraryManager v1.0"
        Width="800" Height="600">

    <StackPanel x:Name="MyStackPanel" VerticalAlignment="Center"> 
        <TextBlock Text="Поиск   " Height="50" HorizontalAlignment="Center">
             <TextBox x:Name="findBox" Width="300" Changed="FindBox_TextChanged"/>
        </TextBlock>
        <ListBox x:Name="ObjectList" SelectionMode="Single" Width="750">
        </ListBox>
        <Button Content="Выбрать" Width="200" Margin="0,20,50,0" HorizontalAlignment="Right" VerticalAlignment="Bottom" HorizontalContentAlignment="Center"/>
    </StackPanel>
</Window>